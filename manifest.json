{
  "manifest_version": 3,
  "name": "eBay Product Scraper",
  "version": "1.1",
  "permissions": ["scripting", "tabs"],
  "host_permissions": [
    "https://www.ebay.com/*",
    "https://script.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.ebay.com/*"],
      "js": ["content.js"]
    }
  ]
}
